{
  "name": "inventory-backend",
  "version": "1.0.0",
  "description": "Inventory management system backend with Express and Sequelize",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
